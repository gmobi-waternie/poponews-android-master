<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="RoundProgressBar">  
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="roundWidth" format="dimension"></attr>
        <attr name="textColor" format="color" />  
        <attr name="textSize" format="dimension" /> 
        <attr name="max" format="integer"></attr> 
        <attr name="textIsDisplayable" format="boolean"></attr>
        <attr name="style">
            <enum name="STROKE" value="0"></enum>
            <enum name="FILL" value="1"></enum>
        </attr>
    </declare-styleable> 
    
    <attr name="bgRedColorValue" format="color" />
    <attr name="bgDarkRedColorValue" format="color" />
    <attr name="bgYellowColorValue" format="color" />
    <attr name="bgGreyColorValue" format="color" />
    <attr name="bgBlackColorValue" format="color" />
    <attr name="bgWhiteColorValue" format="color" />

    
    <attr name="textWhiteColorValue" format="color" />
    <attr name="textBlackColorValue" format="color" />
    <attr name="textGreyColorValue" format="color" />
    
    
    <attr name="floatStyleValue" format="float" />  
    <attr name="integerStyleValue" format="integer" />  
    <attr name="booleanStyleValue" format="boolean" />  
    <attr name="dimensionStyleValue" format="dimension" />  
    <attr name="stringStyleValue" format="string" />  
    <attr name="referenceStyleValue" format="color|reference" />  
    <attr name="imageStyleValue" format="reference"/> 
    
</resources>